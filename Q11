
#include<stdio.h>
 

int main(void)
{
  int n = 8;
  int i;
  int n1;

  printf("Digite um numero inteiro: ");
  scanf("%d", &n);
	n1=n;
  for (i = 2; i < n; i++) {
    while (n % i == 0) {
      n /= i;
      
    }


  }
  if(n==n1)
  {
  		printf("primo \n\n" );
  }
  else{
  		printf("composto \n\n" );
  }
  
  

  return 0;
}
