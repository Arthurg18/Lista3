#include <stdio.h>


int main()
{
	  //declaração de variáveis do tipo inteiro
  int a
  ;
  
  printf("Digite um numero inteiro: ");
  scanf("%d", &a); //recebe um inteiro e armazena na variável a
  
	int i;
for( i = 1; i*i<a;)
{	
	i++;
	
}
	printf(maior inteiro ao quadrado maior que n = %d \n\n", i );
    
    return 0;
}
